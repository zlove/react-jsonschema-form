{
  "name": "@rjsf/playground",
  "version": "2.0.0-alpha.4",
  "description": "rjsf playground",
  "private": true,
  "scripts": {
    "build:lib": "rimraf lib && cross-env NODE_ENV=production babel -d lib/ src/",
    "prepublishOnly": "npm run build:lib",
    "build:dist": "rimraf dist && cross-env NODE_ENV=production webpack --config webpack.config.dist.js",
    "cs-check": "prettier -l \"{playground,src,test}/**/*.js\"",
    "cs-format": "prettier \"{playground,src,test}/**/*.js\" --write",
    "build": "rimraf build && cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "lint": "eslint src test playground",
    "precommit": "lint-staged",
    "publish-to-gh-pages": "npm run build && gh-pages --dist build/",
    "publish-to-npm": "npm run build && npm publish",
    "start": "webpack-dev-server --open --config webpack.config.dev.js"
  },
  "lint-staged": {
    "{playground,src,test}/**/*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "main": "lib/index.js",
  "files": [
    "dist",
    "lib"
  ],
  "engineStrict": false,
  "engines": {
    "npm": ">=2.14.7",
    "node": ">=6"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.4.5",
    "@material-ui/core": "^4.9.4",
    "@rjsf/core": "^2.0.0-alpha.4",
    "@rjsf/material-ui": "^2.0.0-alpha.4",
    "@rjsf/semantic-ui": "^2.0.0-alpha.4",
    "ajv": "^6.7.0",
    "core-js": "^2.5.7",
    "json-schema-merge-allof": "https://github.com/zlove/json-schema-merge-allof",
    "jss": "^10.0.3",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react-app-polyfill": "^1.0.4",
    "react-bootstrap": "^0.33.1",
    "react-frame-component": "^4.1.1",
    "react-is": "^16.9.0",
    "shortid": "^2.2.14"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "@material-ui/icons": "^4.5.1",
    "atob": "^2.0.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "chai": "^3.3.0",
    "codemirror": "^5.30.0",
    "create-react-ref": "^0.1.0",
    "cross-env": "^2.0.1",
    "css-loader": "^0.23.1",
    "eslint": "^4.9.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.4.0",
    "estraverse": "^4.2.0",
    "estraverse-fb": "^1.3.1",
    "express": "^4.14.0",
    "gh-pages": "^0.11.0",
    "html": "^1.0.0",
