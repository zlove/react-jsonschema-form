    "react": "^16.9.0",
    "react-codemirror2": "^4.1.0",
    "react-dom": "^16.9.0",
    "react-monaco-editor": "^0.26.2",
    "react-portal": "^4.2.0",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.3.3",
    "rimraf": "^2.5.4",
    "sass": "^1.23.7",
    "sass-loader": "^7.3.1",
    "sinon": "^7.5.0",
